
# 基本配置
--main=src/main/metadata_editor.py
--output-filename=metadata_editor
--onefile
--assume-yes-for-downloads

# 优化选项
--enable-plugin=anti-bloat
--show-anti-bloat-changes
--nofollow-import-to=tkinter,matplotlib,numpy,scipy,pandas,PIL,cv2,torch,tensorflow,jupyter,IPython,pytest,unittest,doctest

# 包含必要的模块
--include-package=mutagen
--include-module=json
--include-module=argparse
--include-module=base64
--include-module=os
--include-module=sys
--include-module=traceback

# 性能优化
--lto=yes
--jobs=4

# 平台特定配置
# Windows
--windows-console-mode=force

# 调试选项
--show-progress
--show-memory
